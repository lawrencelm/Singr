ENABLE_NS_ASSERTIONS = YES
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/LayerKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/EZAudio" "${PODS_ROOT}/Headers/LayerKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/EZAudio" -isystem "${PODS_ROOT}/Headers/LayerKit"
OTHER_LDFLAGS = -ObjC -lz -framework AVFoundation -framework AudioToolbox -framework CFNetwork -framework GLKit -framework LayerKit -framework MobileCoreServices -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods